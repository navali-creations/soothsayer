{
  "name": "soothsayer",
  "version": "0.0.0",
  "private": true,
  "description": "Soothsayer monorepo",
  "scripts": {
    "app:start": "pnpm --filter @soothsayer/app start",
    "app:start:perf": "pnpm --filter @soothsayer/app start:perf",
    "app:package": "pnpm --filter @soothsayer/app package",
    "app:make": "pnpm --filter @soothsayer/app make",
    "app:tsc": "pnpm --filter @soothsayer/app tsc",
    "scrape:poe1": "pnpm --filter @soothsayer/poe-wiki-scrape scrape:poe1",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "keywords": [],
